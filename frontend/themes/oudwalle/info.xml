<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<name>triton</name>
	<version>1.0.0</version>
	<requirements>
		<minimum_version>3.0.0</minimum_version>
	</requirements>
	<thumbnail>thumbnail.png</thumbnail>
	<description>
		<![CDATA[
			This is the theme for oudwalle.be
		]]>
	</description>
	<authors>
		<author>
			<name>Tom Moerman</name>
			<url>http://www.oudwalle.be</url>
		</author>
	</authors>
	<metanavigation supported="false" />
	<templates>
		<template label="Default" path="core/layout/templates/default.tpl">
			<positions>
				<position name="main" />
				
			</positions>
			<format>
				[main]
			</format>
		</template>
		<template label="Home" path="core/layout/templates/home.tpl">
			<positions>
				<position name="main" />
			</positions>
			<format>
				[main]
			</format>
		</template>	
	</templates>
</theme>
